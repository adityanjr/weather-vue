{
  "name": "weather-vue",
  "label": "WeatherApp",
  "description": "a desktop weather application built with vue and nwjs",
  "version": "1.0.0",
  "author": "Aditya Singh <samant04aditya@gmail.com>",
  "private": true,
  "main": "http://localhost:9080",
  "node-remote": "*://localhost/*",
  "bg-script": "./dist/bg/bg.js",
  "window": {
    "width": 350,
    "height": 600,
    "resizable": false
  },
  "build": {
    "files": [
      "node_modules/**/*",
      "dist/**/*"
    ],
    "output": "./releases",
    "nwVersion": "0.40.1",
    "nwPlatforms": [
      "win"
    ],
    "nwArchs": [
      "x64"
    ],
    "strippedProperties": [
      "label",
      "private",
      "scripts",
      "devDependencies",
      "build"
    ],
    "overriddenProperties": {
      "main": "./dist/main/index.html"
    }
  },
  "scripts": {
    "dev": "node .nwjs-vue/dev-runner.js",
    "build": "node .nwjs-vue/prod-builder.js"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "ajv": "^6.10.2",
    "axios": "^0.21.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "npm-which": "^3.0.1",
    "nwjs-builder-phoenix": "^1.15.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^7.3.1",
    "url-loader": "^2.1.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-router": "^3.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "webpack": "^4.39.2",
    "webpack-dev-server": "^3.8.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
